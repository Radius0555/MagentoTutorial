<?xml version="1.0"?>
<config>
  <modules>
    <SilkSoftware_ModuleName>
      <version>0.1.0</version>
    </SilkSoftware_ModuleName>
  </modules>
  <frontend>
    <routers>
      <modulename>
        <use>standard</use>
          <args>
            <module>SilkSoftware_ModuleName</module>
            <frontName>modulename</frontName>
          </args>
      </modulename>
    </routers>
		<layout>
		  <updates>
			<modulename>
			  <file>modulename.xml</file>
			</modulename>
		  </updates>
		</layout>
  </frontend>
  <global>
	<blocks>
	  <modulename>
		<class>SilkSoftware_ModuleName_Block</class>
	  </modulename>
	</blocks>
  </global>
</config>
